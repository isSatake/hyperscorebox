{
  "name": "hyperscorebox",
  "version": "0.0.6",
  "description": "hyperscore Chrome Extension",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://scrapbox.io/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://scrapbox.io/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style/style.css",
        "style/midi.css"
      ]
    }
  ],
  "browser_action": {
  },
  "manifest_version": 2
}